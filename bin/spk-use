#!/usr/bin/env node

const path = require('path')
const use = require('../lib/use').default
const program = require('commander')

program
  .option('-l, --list', 'List the template\'s contents')
  .option('-s, --safe', 'Don\'t overwrite or delete files')
  .option('--skip-update', 'Skip pre import template update')
  .parse(process.argv)

use(program.args[0], program)
